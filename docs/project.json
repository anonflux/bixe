{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "docs/src",
  "projectType": "library",
  "targets": {
    "start": {
      "executor": "nx:run-script",
      "options": {
        "script": "start",
        "forwardAllArgs": true,
        "cwd": "docs"
      }
    },
    "build": {
      "executor": "nx:run-script",
      "options": {
        "script": "build",
        "forwardAllArgs": true,
        "cwd": "docs"
      }
    },
    "deploy:local": {
      "executor": "nx:run-script",
      "options": {
        "script": "deploy:local",
        "forwardAllArgs": true,
        "cwd": "docs"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "docs/**/*.ts",
          "docs/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "docs/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
